<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.mainPanel {
		transform: scale(1);
	}
	</ui:style>

	<g:HTMLPanel>
		<g:SimpleLayoutPanel ui:field="pogMainPanel" styleName="{style.mainPanel}">
			<g:AbsolutePanel ui:field="pogDrawPanel"></g:AbsolutePanel>
		</g:SimpleLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder>
